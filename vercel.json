{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "client"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/client/$1"
        }
    ]
}